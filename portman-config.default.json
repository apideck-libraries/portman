{
  "version": 1.0,
  "$schema": "https://raw.githubusercontent.com/apideck-libraries/portman/main/src/utils/portman-config-schema.json",
  "tests": {
    "contractTests": [
      {
        "openApiOperation": "*::/*",
        "statusSuccess": {
          "enabled": true
        }
      },
      {
        "openApiOperation": "*::/*",
        "responseTime": {
          "enabled": false,
          "maxMs": 300
        }
      },
      {
        "openApiOperation": "*::/*",
        "contentType": {
          "enabled": true
        }
      },
      {
        "openApiOperation": "*::/*",
        "jsonBody": {
          "enabled": true
        }
      },
      {
        "openApiOperation": "*::/*",
        "schemaValidation": {
          "enabled": true
        }
      },
      {
        "openApiOperation": "*::/*",
        "headersPresent": {
          "enabled": true
        }
      }
    ],
    "contentTests": [],
    "extendTests": [],
    "variationTests": []
  },
  "assignVariables": [],
  "overwrites": [],
  "globals": {
    "collectionPreRequestScripts": [],
    "keyValueReplacements": {},
    "valueReplacements": {},
    "rawReplacements": []
  }
}
